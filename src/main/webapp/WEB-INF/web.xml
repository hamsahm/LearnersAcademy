<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SubjectController</servlet-name>
  	<display-name>SubjectController</display-name>
  	<description></description>
  	<servlet-class>controller.SubjectController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TeacherController</servlet-name>
  	<display-name>TeacherController</display-name>
  	<description></description>
  	<servlet-class>controller.TeacherController</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>SubjectController</servlet-name>
  	<url-pattern>/subjects</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TeacherController</servlet-name>
  	<url-pattern>/teachers</url-pattern>
  </servlet-mapping>
  
</web-app>
